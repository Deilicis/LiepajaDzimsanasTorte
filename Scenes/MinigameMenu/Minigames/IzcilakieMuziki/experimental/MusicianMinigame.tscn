[sub_resource type="GDScript" id=1]
script/source = "extends Control

onready var allKeys = [$KeyAnim,$KeyAnim2,$KeyAnim3,$KeyAnim4]
var currentKeys = []
var combo = 0

var busy = false

func _ready():
	$MidiPlayer.play()
	yield(get_tree().create_timer(0.411), \"timeout\")
	$AudioStreamPlayer.play()
	pass

func play_random():
	var newKeys = []
	var keyCount = randi()%2 + 1
	var freeKeys = allKeys.duplicate() 
	for a in currentKeys:
		freeKeys.erase(a)
	while keyCount > 0:
		keyCount -= 1
		newKeys.push_back(freeKeys[randi()%freeKeys.size()])
	currentKeys = newKeys
	for k in newKeys:
		k.play()
	pass


func _on_MidiPlayer_midi_event(channel, event):
	if event.type == 128 : #key down
		if busy:
			return
		busy = true
		for N in channel.note_on:
			match N:
				71:
					$KeyAnim.play()
				95:
					$KeyAnim3.play()
				83:
					$KeyAnim2.play()
				59:
					$KeyAnim4.play()
		yield(get_tree().create_timer(0.3),\"timeout\")
		busy = false
	pass 
"

